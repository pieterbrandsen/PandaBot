{
  "name": "pansiep",
  "version": "0.0.0",
  "description": "Screeps AI",
  "main": "nm",
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint **/*",
    "lint-fix": "eslint **/* --fix",
    "push-main": "rollup -c --environment DEST:main",
    "push-pserver": "rollup -c --environment DEST:pserver",
    "push-sim": "rollup -c --environment DEST:sim",
    "push-seasonal": "rollup -c --environment DEST:seasonal",
    "test": "jest",
    "test-verbose": "jest --verbose",
    "test-watch": "jest --watch --verbose",
    "test-cov": "jest --coverage",
    "test-only": "jest --watch --coverage --verbose",
    "watch-main": "rollup -cw --environment DEST:main",
    "watch-pserver": "rollup -cw --environment DEST:pserver",
    "watch-sim": "rollup -cw --environment DEST:sim",
    "codecov": "codecov -f coverage/coverage-final.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pieterbrandsen/Pansiep.git"
  },
  "author": "Pieter Brandsen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pieterbrandsen/Pansiep/issues"
  },
  "homepage": "https://github.com/pieterbrandsen/Pansiep#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.27",
    "@types/screeps": "^3.2.1",
    "@types/source-map": "^0.5.7",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "codecov": "^3.8.1",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsdoc": "^31.6.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-screeps": "^2.1.0",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "prettier": "2.2.1",
    "rollup": "^2.39.1",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-screeps": "^1.0.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "lodash": "^3.10.1",
    "screeps-jest": "github:eduter/screeps-jest",
    "source-map": "^0.7.3"
  }
}
